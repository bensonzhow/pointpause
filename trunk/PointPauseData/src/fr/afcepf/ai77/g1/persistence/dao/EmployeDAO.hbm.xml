<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="fr.afcepf.ai77.g1.persistence.dao">
  <class name="EmployeDAO" table="employe">
  	
  	<id name="matricule" type="integer" >
  		<column name="matricule_employe" sql-type="int" not-null="true"/>
  		<generator class="increment"/>
  	</id>
  	
    
    <set name="listeClients" cascade="save-update" >
    	<key column="numero_client" />
    	<one-to-many class="ClientDAO" />
    </set>
  	
  </class>
</hibernate-mapping>